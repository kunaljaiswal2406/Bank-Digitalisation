<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="bank.dto">
    <class name="Loan" table="loanReq">
        <id  name="accNo"  column="accountNo" />
        <property name="name" column="name" />
        <property name="loanType" column="lType" />
        <property name="contact" column="contact" />
        <property name="Addr" column="addr" />
        <property name="others" column="other" />

    </class>
</hibernate-mapping>
